                success: function (res) {

                    if (res.taskSuccess == true) {
                      //  var currChuBodyId = $("#_hdnChuBodyId").val();

                        //var currId = $("#_hdnCurrId_CB").val();
                      //  var _pageIndex = $("#_hdnPageIndex").val();
                        //alert("saving currId: " + currId + " ...page: " + res.pageIndex);

                        var currDenomId = $("#_hdnAppGloOwnId").val();

                        var _desc = "'[New] Church unit'";
                        if (res.resetNew == true) {
                            //DisplaySuccessInfo = function (msg, currDenomId, pageIndex, reload = false, newItemId = null, _hdrdesc='')
                            DisplaySuccessInfo(res.userMess, currDenomId, res.pageIndex, false, 0, _desc);

                            // AddEditCurrData_CB = function (currId, currDenomId, _pageIndex, _strItemName)
                           // AddEditCurrData_CB(0, currDenomId, _pageIndex, _desc); //(0, currDenomId, currChuBodyId, currSetIndex, currSubSetIndex, _desc);
                        }
                        else {
