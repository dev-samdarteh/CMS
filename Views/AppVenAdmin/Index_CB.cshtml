        var DeleteCurrData_CB = function (currId, _pageIndex, forceDeleteConfirm = false, msg = "") {
       // alert('deleting')

       // var currUserId = $("#_oUserId_Logged").val();

        var currUserId_Logged = $("#_oUserId_Logged").data("value");
      //  var currDenomId = $("#_hdnAppGloOwnId").val();
