    <script>

        $(document).on('click', '#btnSaveChanges', function (e) {
            e.preventDefault();

           // alert('click..');

            const form = document.getElementById("currForm_CB");

            if (form.checkValidity() === true) {

                SaveCurr_CB(); //currSetIndex, currSubSetIndex);
            }
            else {
               // alert('val 5')
                event.preventDefault();
